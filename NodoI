public class NodoI <E>{
	
	E dato;
	NodoI <E> siguiente;
	
	public NodoI(){
		
		dato = null;
		siguiente =null;
		
		
	}
	
	public NodoI ( E dato){
		this.dato = dato;
		siguiente = null;
	}

	public E getDato() {
		return dato;
	}

	public void setDato(E dato) {
		this.dato = dato;
	}

	public Nodo<E> getSiguiente() {
		return siguiente;
	}

	public void setSiguiente(Nodo<E> siguiente) {
		this.siguiente = siguiente;
	}
	
	

}
